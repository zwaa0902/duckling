{
  "singleQuote": true,
  "jsxSingleQuote": true,
  "printWidth": 125,
  "tabWidth": 2,
  "semi": true,
  "useTabs": false,
  "trailingComma": "all",
  "arrowParens": "always",
  "endOfLine": "auto",
  "overrides": [
    {
      "files": "*.{js,jsx,tsx,ts,scss,json,html}",
      "options": {
        "tabWidth": 2
      }
    }
  ]
}
